{
  "name": "ts-gulp-boilerplate",
  "version": "0.0.1",
  "description": "A simple Typescript bolierplate with gulp",
  "author": "tizee",
  "license": "MIT",
  "browserslist": [
    "last 2 version, > 0.5%"
  ],
  "scripts": {
    "ts-check": "tsc --noEmit",
    "ts-check:watch": "tsc --noEmit --watch",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint src",
    "format": "prettier src/**/*.{js,ts,jsx,tsx} --write",
    "dev": "cross-env NODE_ENV=development gulp",
    "build": "cross-env NODE_ENV=production gulp build"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.4",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@babel/runtime": "^7.10.4",
    "@types/gulp": "^4.0.6",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.1",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "ejs": "^3.1.3",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "fibers": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-data": "^1.3.1",
    "gulp-ejs": "^5.1.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-less": "^4.0.1",
    "gulp-load-plugins": "^2.0.3",
    "gulp-pug": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-typescript": "^6.0.0-alpha.1",
    "gulp-uglify": "^3.0.2",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.0.5",
    "readable-stream": "^3.6.0",
    "sass": "^1.26.9",
    "ts-loader": "^7.0.5",
    "tsify": "^4.0.2",
    "typescript": "^3.9.6",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.1",
    "webpack": "^5.27.1",
    "webpack-stream": "^5.2.1",
    "winston": "^3.3.3"
  },
  "dependencies": {}
}
